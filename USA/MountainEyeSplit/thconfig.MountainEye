encoding  utf-8
source MountainEye.th
  export map -o export/MountaineyeMap3.pdf -layout Layout2
  export map -proj plan -o mountaineye_Walls.3d
  
  
layout Layout2
  scale 1 1000				    # Sets the scale for / on exported maps. Default is 1: 200.
  base-scale 1 1000
  #Foreground color - turn on altitdude to see various heights
  #colour map-fg [100 100 100]
  color map-fg altitude
  #color-legend on
  colour map-bg [100 100 100]
  #sketches on 
  debug off
endlayout
